<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
     <!-- Whether or not to enable the lock screen entry point for the QR code scanner. -->
     <bool name="config_enableQrCodeScannerOnLockScreen">true</bool>
     <!-- Default web browser.  This is the package name of the application that will
          be
     the default browser when the device first boots.  Afterwards the user
          can select
     whatever browser app they wish to use as the default.
          If this string is empty or the
     specified package does not exist, then
          the behavior will be as though no app was
     named as an explicit default. -->
     <string name="default_browser" translatable="false">org.mozilla.firefox</string>
     <string name="config_defaultBrowser" translatable="false">org.mozilla.firefox</string>
     <!-- Disable network policy warning. -->
     <integer name="config_networkPolicyDefaultWarning">-1</integer>
</resources>